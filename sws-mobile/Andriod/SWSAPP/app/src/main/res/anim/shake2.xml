<set xmlns:android="http://schemas.android.com/apk/res/android"
     android:interpolator="@anim/cycle_2"
    >
    <translate
        android:fromXDelta="0"
        android:toXDelta="-10"
        android:fromYDelta="0"
        android:toYDelta="-10"
        android:duration="300"/>
    <translate
        android:fromXDelta="0"
        android:toXDelta="10"
        android:fromYDelta="0"
        android:toYDelta="-10"
        android:startOffset="300"
        android:duration="300"/>
