        android:fromXDelta="0"
        android:toXDelta="-10"
        android:fromYDelta="0"
        android:toYDelta="10"
        android:startOffset="600"
        android:duration="300"/>
    <translate
        android:fromXDelta="0"
        android:toXDelta="10"
        android:fromYDelta="0"
        android:toYDelta="10"
        android:startOffset="900"
        android:duration="300"/>
</set>